{
  "expo": {
    "name": "pjas-app",
    "slug": "pjas-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#122937"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/PJAS_AdaptiveIcon_Frg.png",
        "backgroundImage": "./assets/images/PJAS_AdaptiveIcon_Bkg.png"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
